  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/adminDashboard/**/*.*"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                "src/index/tenant/*",
                "src/index/tenant/**",
                "src/stores/userAuthStore"
              ]
            }
          ]
        }
      },
      {
        "files": [
          "src/dashboard/**/*.*",
          "src/clientDashboard/**/*.*",
          "src/tenantDashboard/**/*.*"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                "src/index/admin/*",
                "src/index/admin/**",
                "src/stores/adminAuthStore"
              ]
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
