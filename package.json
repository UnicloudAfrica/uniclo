{
  "name": "unicloud",
  "homepage": "http://unicloudafrica.com/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@paystack/inline-js": "^2.22.6",
    "@tanstack/react-query": "^5.81.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tinymce/tinymce-react": "^4.3.2",
    "@xyflow/react": "^12.10.0",
    "axios": "^1.13.2",
    "countries-list": "^3.0.6",
    "country-state-city": "^3.2.1",
    "firebase": "^10.6.0",
    "framer-motion": "^10.16.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "laravel-echo": "^2.2.6",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.511.0",
    "papaparse": "^5.5.3",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.5",
    "react-loading-skeleton": "^3.5.0",
    "react-paystack": "^6.0.0",
    "react-pdf": "^10.0.1",
    "react-router-dom": "^6.18.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.6.0",
    "sonner": "^2.0.5",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.5"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "src/adminDashboard/**/*.*"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                "src/index/tenant/*",
                "src/index/tenant/**",
                "src/stores/tenantAuthStore"
              ]
            }
          ]
        }
      },
      {
        "files": [
          "src/dashboard/**/*.*",
          "src/clientDashboard/**/*.*",
          "src/tenantDashboard/**/*.*"
        ],
        "rules": {
          "no-restricted-imports": [
            "error",
            {
              "patterns": [
                "src/index/admin/*",
                "src/index/admin/**",
                "src/stores/adminAuthStore"
              ]
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/register": "^7.25.9",
    "@faker-js/faker": "^10.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "faker": "^6.6.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "msw": "^2.11.5",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  }
}
